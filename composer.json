{
  "name": "weberstudio/clips",
  "description": "CLiPs Early Educational Student Profiles",
  "version": "0.0.1",
  "keywords": [],
  "license": "proprietary",
  "homepage": "http://kedweber.github.io",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Ked Weber",
      "email": "not@weberstudio.net",
      "homepage": "http://www.weberstudio.net"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
  ],
  "autoload": {
    "Clips\\": "module/Clips/src"
  },
  "require": {
    "php": ">=5.3.0 || ^7.0",
    "zendframework/zendframework": "2.3.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-orm-module": "0.*"
  },
  "autoload-dev": {
    "ClipsTest\\": "module/Clips/test/"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*",
    "phpunit/phpunit-dom-assertions": "1.0.*@dev",
    "phpunit/php-invoker": "^1.1",
    "zendframework/zend-developer-tools": "^1.0"
  },
  "scripts": {
    "self-serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
  },
  "config": {
    "discard-changes": true
  }
